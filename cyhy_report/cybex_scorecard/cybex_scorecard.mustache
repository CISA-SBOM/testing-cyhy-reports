    \settowidth{\imgwidth}{\includegraphics[scale=#1]{#2}}
    \includegraphics[width=\imgwidth]{#2}
	\caption{#3}
	\label{fig:#2}
}

\newcommand\scalegraphicsupnofig[2][1.0]{
